#!/usr/bin/bash

a=$2

./run 10.28.1.2 $1 $(seq -s , $((a * 1)) $((a * 1 + a - 1))) >/dev/null 2>&1 &
./run 10.28.1.3 $1 $(seq -s , $((a * 2)) $((a * 2 + a - 1))) >/dev/null 2>&1 &
./run 10.28.1.4 $1 $(seq -s , $((a * 3)) $((a * 3 + a - 1))) >/dev/null 2>&1 &
./run 10.28.1.5 $1 $(seq -s , $((a * 4)) $((a * 4 + a - 1))) >/dev/null 2>&1 &
./run 10.28.1.6 $1 $(seq -s , $((a * 5)) $((a * 5 + a - 1))) >/dev/null 2>&1 &
./run 10.28.1.7 $1 $(seq -s , $((a * 6)) $((a * 6 + a - 1))) >/dev/null 2>&1 &
./run 10.28.1.8 $1 $(seq -s , $((a * 7)) $((a * 7 + a - 1))) >/dev/null 2>&1 &

./run 10.28.1.1 $1 $(seq -s , 0 $((a - 1)))
