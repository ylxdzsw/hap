#!/bin/bash

a=$2

./run v2 $1 $(seq -s , $((a * 1)) $((a * 1 + a - 1))) >/dev/null 2>&1 &
./run v3 $1 $(seq -s , $((a * 2)) $((a * 2 + a - 1))) >/dev/null 2>&1 &
./run v4 $1 $(seq -s , $((a * 3)) $((a * 3 + a - 1))) >/dev/null 2>&1 &
./run v5 $1 $(seq -s , $((a * 4)) $((a * 4 + a - 1))) >/dev/null 2>&1 &
./run v6 $1 $(seq -s , $((a * 5)) $((a * 5 + a - 1))) >/dev/null 2>&1 &
./run v7 $1 $(seq -s , $((a * 6)) $((a * 6 + a - 1))) >/dev/null 2>&1 &
./run v8 $1 $(seq -s , $((a * 7)) $((a * 7 + a - 1))) >/dev/null 2>&1 &

./run v1 $1 $(seq -s , 0 $((a - 1)))
